<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DoctorTravelCertificateController">
   <children>
      <TitledPane animated="false" text="Travel Certificate : Doctor">
        <content>
          <AnchorPane prefHeight="520.0" prefWidth="590.0" style="-fx-background-color: #AFE1AF;" stylesheets="@style.css">
               <children>
                  <TableView fx:id="certificateTable" layoutX="35.0" layoutY="19.0" prefHeight="359.0" prefWidth="522.0">
                    <columns>
                      <TableColumn fx:id="certificateIdColumn" prefWidth="71.0" text="Checkup#" />
                      <TableColumn fx:id="usernameColumn" prefWidth="150.0" text="Username" />
                        <TableColumn fx:id="animalNameColumn" prefWidth="153.0" text="Animal Name" />
                        <TableColumn fx:id="checkupStatusColumn" prefWidth="146.0" text="Checkup Status" />
                    </columns>
                  </TableView>
                  <Button layoutX="380.0" layoutY="411.0" mnemonicParsing="false" onMouseClicked="#handleApproveButton" prefHeight="50.0" prefWidth="177.0" text="APPROVE">
                     <font>
                        <Font name="Century Gothic" size="14.0" />
                     </font></Button>
                  <Button layoutX="35.0" layoutY="411.0" mnemonicParsing="false" onMouseClicked="#handleBackButton" prefHeight="50.0" prefWidth="176.0" text="BACK">
                     <font>
                        <Font name="Century Gothic" size="14.0" />
                     </font></Button>
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</AnchorPane>
