                     <font>
                        <Font name="Century Gothic" size="14.0" />
                     </font></Button>
                  <Button layoutX="198.0" layoutY="434.0" mnemonicParsing="false" onMouseClicked="#handleCheckupButton" prefHeight="58.0" prefWidth="195.0" text="CHECKUP">
                     <font>
                        <Font name="Century Gothic" size="14.0" />
                     </font></Button>
                  <Label layoutX="187.0" layoutY="55.0" prefHeight="23.0" prefWidth="214.0" style="-fx-background-color: #ECFFDC;" text="Name">
