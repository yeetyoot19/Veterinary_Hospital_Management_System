<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DoctorMainController">
   <children>
      <TitledPane animated="false" text="DoctorMain">
        <content>
          <AnchorPane prefHeight="520.0" prefWidth="590.0" style="-fx-background-color: #AFE1AF;" stylesheets="@style.css">
               <children>
                  <Button layoutX="49.0" layoutY="320.0" mnemonicParsing="false" onMouseClicked="#handleAppointmentsButton" prefHeight="59.0" prefWidth="195.0" text="APPOINTMENTS">
